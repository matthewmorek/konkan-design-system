{
  "core": {
    "const": {
      "baseline-grid-unit": {
        "value": "4*{core.const.platform-scale}",
        "type": "sizing"
      },
      "rounding-unit": {
        "value": "{core.const.baseline-grid-unit}/2",
        "type": "sizing"
      },
      "platform-scale": {
        "value": "1",
        "type": "sizing"
      }
    },
    "space": {
      "2xsmall": {
        "value": "{core.const.baseline-grid-unit}",
        "type": "spacing"
      },
      "xsmall": {
        "value": "{core.const.baseline-grid-unit}*2",
        "type": "spacing"
      },
      "small": {
        "value": "{core.const.baseline-grid-unit}*3",
        "type": "spacing"
      },
      "medium": {
        "value": "{core.const.baseline-grid-unit}*4",
        "type": "spacing"
      },
      "large": {
        "value": "{core.const.baseline-grid-unit}*6",
        "type": "spacing"
      },
      "xlarge": {
        "value": "{core.const.baseline-grid-unit}*8",
        "type": "spacing"
      },
      "2xlarge": {
        "value": "{core.const.baseline-grid-unit}*16",
        "type": "spacing"
      },
      "inset": {
        "2xsmall": {
          "value": "{core.space.2xsmall} {core.space.2xsmall}",
          "type": "spacing"
        },
        "xsmall": {
          "value": "{core.space.xsmall} {core.space.xsmall}",
          "type": "spacing"
        },
        "small": {
          "value": "{core.space.small} {core.space.small}",
          "type": "spacing"
        },
        "medium": {
          "value": "{core.space.medium} {core.space.medium}",
          "type": "spacing"
        },
        "large": {
          "value": "{core.space.large} {core.space.large}",
          "type": "spacing"
        },
        "xlarge": {
          "value": "{core.space.xlarge} {core.space.xlarge}",
          "type": "spacing"
        },
        "2xlarge": {
          "value": "{core.space.2xlarge} {core.space.2xlarge}",
          "type": "spacing"
        }
      }
    },
    "layout-grid": {
      "margins": {
        "value": "0 {core.space.small}",
        "type": "spacing"
      },
      "gutters": {
        "value": "0 {core.space.small}",
        "type": "spacing"
      },
      "columns": {
        "value": "4",
        "type": "spacing"
      },
      "max-width": {
        "value": "{breakpoint.viewport.max}",
        "type": "spacing",
        "description": "Override this to set a max-width for the grid at larger breakpoints"
      }
    }
  }
}