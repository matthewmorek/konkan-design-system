              "fontFamily": "{core.typography.font-family.plain}",
              "fontWeight": "{core.typography.font-weight.regular}",
              "lineHeight": "{core.grid.const.base-grid-unit} * ceil( {core.typography.font-size.100}*{core.typography.line-height.relaxed} / {core.grid.const.base-grid-unit} )",
